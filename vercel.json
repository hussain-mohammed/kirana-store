{
  "version": 2,
  "builds": [
    {
      "src": "main.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "main.py"
    }
  ],
  "regions": ["sin1"],
  "buildCommand": "pip install -r requirements.txt",
  "devCommand": "python main.py",
  "installCommand": "pip install -r requirements.txt"
}
