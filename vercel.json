{
  "version": 2,
  "builds": [
    {
      "src": "index.html",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "index.html"
    }
  ],
  "functions": {
    "index.html": {
      "maxDuration": 10
    }
  },
  "regions": ["sin1"],
  "buildCommand": "echo 'Static build complete'"
}
